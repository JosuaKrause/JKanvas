{
    "title": {
        "type": "jkanvas.painter.TitleRenderpass",
        "args": "parent,title,height,space",
        "title": "",
        "height": "40",
        "space": "5",
    },
    "handler": {
        "ids": "canvas",
        "type": "jkanvas.DefaultMessageHandler",
        "args": "frame",
        "frame": "#frame",
    },
    "help": {
        "ids": "info",
        "type": "jkanvas.painter.SimpleTextHUD",
        "args": "hpos,vpos",
        "hpos": "jkanvas.painter.TextHUD.RIGHT",
        "vpos": "jkanvas.painter.TextHUD.BOTTOM",
    },
    "canvas": {
        "background": "java.awt.Color.WHITE",
        "help": {
            "template": "help",
        },
        "handler": {
            "template": "handler",
        },
    },
}
